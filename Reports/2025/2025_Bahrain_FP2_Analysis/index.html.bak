    <script>
        function highlightDriver(driver) {
            // Reset all
            document.querySelectorAll('.trace-line polyline').forEach(el => {
                el.classList.add('dimmed');
                el.classList.remove('highlighted');
            });

            // Highlight targets (in all charts)
            const targets = document.querySelectorAll('.trace-line-' + driver + ' polyline');
            targets.forEach(target => {
                target.classList.remove('dimmed');
                target.classList.add('highlighted');
                // Move to top (SVG z-index hack: re-append to ITS OWN parent)
                const g = target.parentNode;
                g.parentNode.appendChild(g);
            });
        }

        function resetHighlight() {
            document.querySelectorAll('.trace-line polyline').forEach(el => {
                el.classList.remove('dimmed');
                el.classList.remove('highlighted');
            });
        }
    </script>
