            background: #E80020;
            color: white;
            padding: 2px 5px;
            border-radius: 4px;
            font-size: 0.7em;
            margin-left: 5px;
            vertical-align: middle;
        }

        /* Interactive Chart */
        .trace-line polyline {
            transition: all 0.2s ease;
        }

        .trace-line:hover polyline {
            stroke-width: 4 !important;
            opacity: 1 !important;
        }

        .dimmed {
            opacity: 0.1 !important;
        }

        .highlighted {
            stroke-width: 4 !important;
            opacity: 1 !important;
            z-index: 100;
        }

        .insight-box {
            border-left: 4px solid #3671C6;
            background: rgba(54, 113, 198, 0.1);
            padding: 15px;
            margin-top: 20px;
            border-radius: 4px;
        }

